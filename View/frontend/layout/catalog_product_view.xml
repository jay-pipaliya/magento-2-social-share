<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\SocialShare\Block\FloatDisplay" name="mp.SocialShare.InlineTop" template="Mageplaza_SocialShare::InlineTop.phtml" before="-"/>
        </referenceContainer>

        <referenceContainer name="content">
            <referenceContainer name="product.info.main">
                <block class="Mageplaza\SocialShare\Block\FloatDisplay" name="mp.SocialShare.InlineCart" template="Mageplaza_SocialShare::InlineCart.phtml"/>
            </referenceContainer>
        </referenceContainer>

        <referenceContainer name="content.aside">
            <block class="Mageplaza\SocialShare\Block\FloatDisplay" name="mp.SocialShare.InlineBottom" template="Mageplaza_SocialShare::InlineBottom.phtml"/>
        </referenceContainer>
    </body>
</page>
